.border-line{
	display: block;
	width:30px; height:2px; margin-top:5px;
	background:#f97e79;

	&.short{
		width:20px; margin-top:3px;
	}

	&.orange{
		background:#f97e79;
	}
	&.green{
		background:#00a992;
	}
	&.white{
		background:#fff;
	}
}

a.btn-boxy{
	margin-top: 30px; background:#fff;
	border:1px solid #00A990; color:#00A990;
	font-size:18px; text-transform:uppercase; letter-spacing:1px;
	line-height:48px; height:48px; padding:0 20px;
	@include inline-block;
	@include box-shadow(3px 3px 0px 0, #019882);

	&:hover{
		text-decoration: none; background:#fbfbfb;
	}

	&.view-all{
		font-size: 14px; line-height:36px; height:36px;
		margin-top:10px;

		@include if-device(phone) {
			font-size: 12px; line-height:30px; height:30px;
			padding:0 10px; margin-top:4px;
			@include box-shadow(0px 0px 0px 0, #019882);
		}
	}
}

/*** Main Header ***/

.main-header{
	height:60px; background:#fff;
	.container{
		padding:0;

		.logo{
			display: block; float:left;

			width: 156px;
			height: 60px;
			vertical-align: baseline; zoom: 1;
			background-repeat: no-repeat;
			background-position: center center;
			background-image: url("../images/logo_small.png");
			text-indent: 100%;
			white-space: nowrap;
			overflow: hidden !important;

			h1{
				*display: none;
			}
		}

		a.btn-boxy.buy-tickets{
			float: right; margin-top:14px;
			font-size: 12px; padding:0 15px; height:32px; line-height:32px;
			background:#fff; border:1px solid #00A990; color:#00A990;
			@include box-shadow(3px 3px 0px 0, #019882);

			&:hover{
				background:#e5f6f3;
			}
		}

		.top-navigation{
			float: right; margin-top:24px;
			ul{
				list-style: none;
				li{
					@include inline-block; float: left; margin-right: 40px;
					
					a{
						display:block; height:12px; margin:0;
						font-size: 12px; line-height:12px; font-weight: bold;
						text-transform: uppercase; letter-spacing:1px;

						&:hover{
							text-decoration: none;
						}
					}
					.border-line{
						display:none;
					}

					&.active,
					&:hover{
						.border-line{
							@include inline-block; margin-top: 5px;
						}
					}

					&.speakers{
						a{	color:#00a992;	}
						.border-line{ background:#00a992; }
					}
					&.schedule{
						a{	color:#fca349;	}
						.border-line{ background:#fca349; }
					}
					&.venue{
						a{	color:#6799b5;	}
						.border-line{ background:#6799b5; }
					}
					&.sponsors{
						a{	color:#414954;	}
						.border-line{ background:#414954; }
					}
					&.blog{
						a{	color:#4c657d;	}
						.border-line{ background:#4c657d; }
					}
				}
			}

			@include if-device(phone){
				display: none;
			}
		}
	}

	@include if-device(phone-tablet){
		.container{
			padding-left:5%; padding-right:5%;
		}
	}
}

.main-header-home{
	background:url(../images/header_bttm_border.png) repeat-x bottom;
	overflow:hidden;

	.bg-trianlge{
		width: 0px;
		height: 0px;
		border-style: solid;
		border-width: 608px 0px 0 0;
		border-color: #fbfbfb transparent transparent transparent;
		line-height: 0px;
		_border-color: #fbfbfb #000000 #000000 #000000;
		_filter: progid:DXImageTransform.Microsoft.Chroma(color='#000000');

		position:absolute; z-index:1;
	}

	.container{
		padding:50px 0; position:relative; z-index:2; 
		background-repeat: no-repeat; background-position: bottom right;
		@include bg-image-2x("../images/header_illustration", png, 299px, 287px);
	}

	.logo{
		margin-bottom: 40px;
	}

	.desc{
		width:65%; color:#f97e79; font-size:20px;
	}

	.events-info{
		margin-top: 30px;

		.event-item{
			color:#f97e79;
			float: left; margin-right:45px;
			.name{
				font-size: 20px; line-height:24px; font-weight: bold;
			}
			.date{
				font-size:18px; line-height:20px;
			}
		}
	}

	a.btn-boxy.buy-tickets{
		background:#fff; border:1px solid #00A990; color:#00A990;
		@include box-shadow(3px 3px 0px 0, #019882);

		&:hover{
			background:#e5f6f3;
		}
	}

	.top-navigation{
		float: left; margin-left: 30px;
		ul{
			list-style: none;
			li{
				@include inline-block; float: left; position: relative; margin-right: 5px;
				
				a{
					display:block; line-height: 48px; border: 1px solid transparent;
					background: none; padding: 0px 24px;
					font-size: 14px; font-weight: bold;
					text-transform: uppercase; color: #4A494A;
					@include border-radius(3px);

					&:hover{
						text-decoration: none; background: rgba(0,0,0,0.05);
					}
				}
				.active-icon{
					position: absolute; width: 100%;
				}
				&.active{
					a{
						background: #fff;
						border: 1px solid $primary-border-color;

						&:after{
							content:""; position: absolute; bottom:0; height: 10px; width: 20px;
							display: inline-block;
							left: 50%; margin-left: -10px;
							background: url(../images/navigation_active_traingle_icon.png) no-repeat bottom center;
						}
					}
				}
			}
		}
	}

	@include if-device(phone-tablet){
		.logo{
			max-width:80%;
		}
		.container{
			padding-left:5%; padding-right:5%;
		}
		.desc{
			width:65%; font-size:18px;
		}
	}

	@include if-device(phone){
		.bg-trianlge{
			display: none;
		}
		.container{
			background:none; padding:20px 5%;
		}
		.logo{
			margin-bottom: 20px;
		}
		.desc{
			width:auto; font-size:14px;
		}
		.events-info{
			margin-top: 20px;

			.event-item{
				margin-right:25px;
				.name{
					font-size: 18px; line-height:22px;
				}
				.date{
					font-size:16px; line-height:18px;
				}
			}
		}
		a.btn-boxy.buy-tickets{
			@include box-shadow(0px 0px 0px 0, #019882);
			font-size: 14px; height:36px; line-height:36px;
			padding:0 10px; margin-top:20px;
		}
	}
}

.page-heading{
	background-color:#C04448; width:100%;
	background-repeat:repeat;

	.container{
		padding-top:30px;
		padding-bottom:30px;
	}

	h1{
		padding-left:70px; height:60px; line-height:60px;
		font-size: 48px; color:#fff; margin:0;
		background-repeat:no-repeat; background-position:left center;
	}

	&.schedule{
		background-color:#EB6566;
		background-image:url(../images/bg_page_heading_schedule.png);
		h1{
			background-image:url(../images/page_icon_schedule.png);
		}
	}

	&.speakers{
		background-color:#378F76;
		background-image:url(../images/bg_page_heading_speakers.png);
		h1{
			background-image:url(../images/speakers_icon.png);
		}
	}

	&.blog{
		background-color:#43556C;
		background-image:url(../images/bg_page_heading_blog.png);
		h1{
			background-image:url(../images/page_icon_blog_white.png);
		}
	}

	&.venue{
		background-color:#5F819E;
		background-image:url(../images/bg_page_heading_venue.png);
		h1{
			background-image:url(../images/page_icon_venue.png);
		}
	}

	@include if-device(phone){
		background-size:auto 70px;

		.container{
			padding-top:15px;
			padding-bottom:15px;
		}
		h1{
			padding-left:50px; height:40px; line-height:40px;
			font-size: 32px;
			background-size:auto 40px;
		}
	}
}



.disable-text-select {
	@include disable-text-select();
}

 
.main-footer{
	overflow:hidden;

	.bg-trianlge{
		position:absolute; z-index:1;

		width: 0px;
		height: 0px;
		border-style: solid;
		border-width: 0 0 520px 0px;
		border-color: transparent transparent #fbfbfb transparent;
		line-height: 0px;
		_border-color: #000000 #000000 #fbfbfb #000000;
		_filter: progid:DXImageTransform.Microsoft.Chroma(color='#000000');

		@include if-device(phone){
			display: none; 
		}
	}

	.footer-container{
		position:relative; z-index:2; 
		height: 80px; padding-top: 440px; // Should be 520px total
		text-align: center; color:#69492e;
		font-size: 14px;

		background-repeat: no-repeat; background-position: center bottom;
		@include bg-image-2x("../images/footer_coder_illustration", png, 534px, 450px);

		@include if-device(phone-tablet){
			background-size:auto 380px; height: 60px; padding-top: 380px;
		}

		@include if-device(phone){
			background:#f8f8f8; color:#6d6d6d; font-size:12px;
			padding-top: 20px;padding-bottom: 20px; height:auto;
		}
	}
}

.hexagon {
    overflow: hidden;
    visibility: hidden;
    -webkit-transform: rotate(120deg);
       -moz-transform: rotate(120deg);
         -o-transform: rotate(120deg);
            transform: rotate(120deg);
    cursor: pointer;
    }
.hexagon-in1 {
    overflow: hidden;
    width: 100%;
    height: 100%;
    -webkit-transform: rotate(-60deg);
       -moz-transform: rotate(-60deg);
         -o-transform: rotate(-60deg);
            transform: rotate(-60deg);
    }
.hexagon-in2 {
    width: 100%;
    height: 100%;
    background-repeat: no-repeat;
    background-position: 50%;
    //background-image: url(http://placekitten.com/240/240);
    background-size:250px auto;
    visibility: visible;
    -webkit-transform: rotate(-60deg);
       -moz-transform: rotate(-60deg);
         -o-transform: rotate(-60deg);
            transform: rotate(-60deg);
    }
.hexagon-in2:hover {
    //background-image: url(http://placekitten.com/241/241);
}

.hexagon1 {
    width: 400px;
    height: 200px;
    margin: 0 0 0 -80px;
    }
.hexagon2 {
    width: 200px;
    height: 230px;
    margin: -0px 0 0 00px;
    }
.dodecagon {
    width: 200px;
    height: 200px;
    margin: -80px 0 0 00px;
    }

@include if-device(phone-tablet){

	.thumb.home{
		.hexagon2 {
		    width: 150px;
		    height: 172px;
		    margin: 0;
		}
		.hexagon-in2 {
			background-size:190px auto;
	    }
	}
}

@include if-device(phone){

	.thumb,
	.thumb.home{
		.hexagon2 {
		    width: 100px;
		    height: 115px;
		    margin: 0;
		}
		.hexagon-in2 {
			background-size:125px auto;
	    }
	}
}



